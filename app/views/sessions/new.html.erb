<h1>ログイン</h1>
  <%= form_with(url: login_path, scope: :session, local: true) do |f|%>
   <div>
    <%= f.label :email,"メールアドレス" %>
    <%= f.email_field :email %>
    </div>
    <div>
    <%= f.label :password,"パスワード" %>
    <%= f.password_field :password %>
    </div>
    <%= f.submit "ログイン" %>
    </div>
<%end%>

<p>新規ユーザ登録は<%= link_to "こちら", signup_url %></p>
